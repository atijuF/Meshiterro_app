<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <% if @user == current_user %>
    <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>

<% @lists.each do |list| %>
<div>
  <%= link_to list_path(list.id) do %>
    <%= image_tag list.get_image %>
  <% end %>
  <p>投稿ユーザー画像：<%= image_tag list.user.get_profile_image(100,100) %></p>
  <p>ショップ名：<%= list.shop_name %></p>
  <p>説明：<%= list.caption %></p>
  <p>ユーザーネーム：<%= list.user.name %></p>
</div>
<% end %>